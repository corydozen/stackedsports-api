<p id="notice"><%= notice %></p>

<h1>Sms Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sms_events.each do |sms_event| %>
      <tr>
        <td><%= link_to 'Show', sms_event %></td>
        <td><%= link_to 'Edit', edit_sms_event_path(sms_event) %></td>
        <td><%= link_to 'Destroy', sms_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

